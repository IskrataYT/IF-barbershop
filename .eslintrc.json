{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "no-console": "off",
        "prettier/prettier": [
            "warn",
            {
              "semi": false,
              "singleQuote": true,
              "trailingComma": "es5",
              "endOfLine": "auto",
              "printWidth": 90
            }
          ],
    "jsx-a11y/anchor-is-valid": "off",
    "react/jsx-props-no-spreading": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-filename-extension": "off",
    "linebreak-style": "off",
    "import/prefer-default-export": "off",
    "semi": ["error", "never"],
    "import/extensions": "off",
    "react/require-default-props": "off",
    "no-unused-vars": "off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
    }
